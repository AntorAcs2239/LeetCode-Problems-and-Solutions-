class Solution {
public:
    vector<int> findSmallestSetOfVertices(int n, vector<vector<int>>& edges) {
        int parent[n+5];
        for(int i=0;i<=n;i++)parent[i]=0;
        int e=edges.size();
        for(int i=0;i<e;i++)
        {
            int u=edges[i][0];
            int v=edges[i][1];
            parent[v]=1;
        }
        vector<int>ans;
        for(int i=0;i<n;i++)
        {
            if(parent[i]==0)
            {
                ans.push_back(i);
            }
        }
        return ans;
    }

};