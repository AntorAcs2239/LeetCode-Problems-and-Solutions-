class Solution {
public:
    int findJudge(int n, vector<vector<int>>& trust) {
        vector<int>graph[n+5];
        vector<int>graph2[n+5];
        int e=trust.size();
        for(int i=0;i<trust.size();i++)
        {
            int u=trust[i][0];
            int v=trust[i][1];
            graph[u].push_back(v);
            graph2[v].push_back(u);
        }
        int ans=-1;
        for(int i=1;i<=n;i++)
        {
            if(graph[i].size()==0&&graph2[i].size()==n-1)
            {
                ans=i;
                break;
            }
        }
        return ans;
    }
};