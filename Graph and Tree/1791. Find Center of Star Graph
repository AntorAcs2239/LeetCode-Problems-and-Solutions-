class Solution {
public:
    int findCenter(vector<vector<int>>& edges) {
        int n=edges.size();
        vector<int>graph[n+5];
        for(int i=0;i<n;i++)
        {
            int a=edges[i][0];
            int b=edges[i][1];
            graph[a].push_back(b);
            graph[b].push_back(a);
        }
        int ans;
        for(int i=1;i<=n;i++)
        {
            if(graph[i].size()==n)
            {
                ans=i;
                break;
            }
        }
        return ans;
    }
};